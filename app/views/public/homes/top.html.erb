<h>aaaa</h>

<div class="genre_search">
  <table class="table">
    
    <div class="row">
          <%= link_to "ゲストログイン（閲覧用）", customers_guest_sign_in_path, class: "btn btn-secondary btn-sm btn-block mb-3 sign_in", method: :post %>
        </div>
    
    <tr>
      <th>ジャンル検索</th>
    </tr>

    <%= form_with url: public_genre_search_path, method: :get do |f| %>
      <% @genres.each do |genre| %>
        <tr>
          <td><%= link_to genre.name, public_genre_search_path(genre_id: genre.id), method: :get %></td>
        </tr>
      <% end %>
    <% end %>

  </table>
</div>